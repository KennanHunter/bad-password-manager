{
	"package": {
		"productName": "bad-password-manager-frontend",
		"version": "0.1.0"
	},
	"build": {
		"distDir": "../public",
		"devPath": "http://localhost:5000",
		"beforeDevCommand": "",
		"beforeBuildCommand": ""
	},
	"tauri": {
		"bundle": {
			"active": true,
			"targets": "all",
			"resources": [],
			"externalBin": [],
			"copyright": "MIT",
			"category": "DeveloperTool",
			"shortDescription": "Password Manager",
			"longDescription": "Password Manager",
			"deb": {
				"depends": [],
				"useBootstrapper": false
			},
			"macOS": {
				"frameworks": [],
				"minimumSystemVersion": "",
				"useBootstrapper": false,
				"exceptionDomain": "",
				"signingIdentity": null,
				"entitlements": null
			},
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"updater": {
			"active": false
		},
		"allowlist": {
			"all": false
		},
		"windows": [
			{
				"title": "Bad Password Manager",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
		}
	}
}
